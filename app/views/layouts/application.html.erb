<!DOCTYPE html>
<html>
  <head>
    <title>Gnizamaquotes</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header class="row">
      <div class="col-3">
        FPOLOGO
      </div>
      <div class="col-4">
        A place to find and add your favorite animation or anime quotes!
      </div>
      <div class="col-5">
        <%= link_to 'Contribute', '#' %>
        <%= link_to 'About', '#' %>
      </div>
    </header>

    <br class="clear" />

    <%= yield %>
  </body>
</html>
